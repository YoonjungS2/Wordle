      <%-include('header.ejs')%>
      <%-include(pageName)%>
      <%-include('footer.ejs')%>
    <!-- include : 다른 템플릿 파일을 현재 템플릿에 삽입 -->
    <!-- 현재 페이지에 header, pageName(home), footer 포함 -->
  </div>
  </body>
</html>

